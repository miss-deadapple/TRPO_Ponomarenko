<Window x:Class="Tri_chisla.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tri_chisla"
        mc:Ignorable="d" Height="450" Width="800" Title="Три числа">
    <Grid>
        <TextBox Name="text1" HorizontalAlignment="Left" Height="23" Margin="10,15,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" PreviewTextInput="TextBox_PreviewTextInput"/>
        <TextBox Name="text2" HorizontalAlignment="Left" Height="23" Margin="10,43,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" PreviewTextInput="TextBox_PreviewTextInput_2"/>
        <TextBox Name="text3" HorizontalAlignment="Left" Height="23" Margin="10,71,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" PreviewTextInput="TextBox_PreviewTextInput_3"/>
        <Button Content="Вычислить" HorizontalAlignment="Left" Margin="10,99,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
        <Label Name="sum" HorizontalAlignment="Left" Margin="10,124,0,0" VerticalAlignment="Top"/>
        <Label Name="multiplier" HorizontalAlignment="Left" Margin="10,155,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
