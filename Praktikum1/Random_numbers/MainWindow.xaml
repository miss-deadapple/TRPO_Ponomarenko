        <TextBox KeyDown="TextBox1_KeyDown" x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="67,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox KeyDown="TextBox1_KeyDown" x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="286,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
