        <Label x:Name="label_Copy" HorizontalAlignment="Left" Margin="315,181,0,0" VerticalAlignment="Top" Height="31" Width="92" RenderTransformOrigin="0.858,-0.131"/>
        <Label x:Name="label_Copy1" HorizontalAlignment="Left" Margin="188,181,0,0" VerticalAlignment="Top" Height="31" Width="92"/>
        <Button  Content="Получить три числа" HorizontalAlignment="Left" Margin="151,125,0,0" VerticalAlignment="Top" Width="191" Height="25" Click="Button_Click"/>
        <Label x:Name="label_Copy2" HorizontalAlignment="Left" Margin="188,272,0,0" VerticalAlignment="Top" Height="31" Width="92"/>

    </Grid>
